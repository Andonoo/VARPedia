<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wikiSpeakController.VideoPlayerController">
   <children>
      <MediaView fx:id="_videoDisplay" fitHeight="360.0" fitWidth="480.0" layoutX="260.0" layoutY="81.0" nodeOrientation="INHERIT" />
      <HBox layoutX="370.0" layoutY="467.0" prefHeight="46.0" prefWidth="92.0">
         <children>
            <Button fx:id="_playButton" mnemonicParsing="false" onAction="#handlePlay" prefHeight="26.0" prefWidth="72.0" text="Play" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <Text fx:id="_creationTitle" layoutX="69.0" layoutY="51.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" wrappingWidth="862.0">
         <font>
            <Font size="35.0" />
         </font>
      </Text>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleBack" text="Back" />
      <Slider fx:id="_playSlider" layoutX="260.0" layoutY="451.0" prefHeight="16.0" prefWidth="480.0" />
      <Slider fx:id="_volumeSlider" layoutX="540.0" layoutY="482.0" />
   </children>
</AnchorPane>
