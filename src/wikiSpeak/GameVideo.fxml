<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wikiSpeakController.GameVideoController">
   <children>
      <MediaView fx:id="_videoDisplay" fitHeight="240.0" fitWidth="320.0" layoutX="340.0" layoutY="180.0" nodeOrientation="INHERIT" />
      <HBox layoutX="410.0" layoutY="522.0" prefHeight="46.0" prefWidth="180.0">
         <children>
            <Button fx:id="_playButton" mnemonicParsing="false" onAction="#handlePlay" prefWidth="80.0" text="Play" />
            <Button mnemonicParsing="false" onAction="#handlePause" prefWidth="80.0" text="Pause" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <Text fx:id="_creationTitle" layoutX="69.0" layoutY="51.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" wrappingWidth="862.0">
         <font>
            <Font size="35.0" />
         </font>
      </Text>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleBack" text="Back" />
   </children>
</AnchorPane>
